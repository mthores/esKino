<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.View.LoginSalMainmenuController">
   <center>
      <HBox alignment="TOP_CENTER" prefHeight="278.0" prefWidth="599.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <ComboBox fx:id="movieCB" prefWidth="150.0" promptText="Vælg Film" />
            <ComboBox fx:id="showCB" prefHeight="27.0" prefWidth="186.0" promptText="Vælg Forestilling" />
         </children>
         <padding>
            <Insets top="50.0" />
         </padding>
      </HBox>
   </center>
   <top>
      <VBox alignment="TOP_CENTER" BorderPane.alignment="CENTER">
         <children>
            <Label text="Reservér">
               <font>
                  <Font size="40.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <Label fx:id="warninglabel" textFill="#ff0707" />
         </children>
      </VBox>
   </top>
   <bottom>
      <HBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#toMenuButtonClicked" text="Tilbage" />
            <Region prefHeight="31.0" prefWidth="459.0" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#reserveButtonClicked" text="Vælg" textAlignment="CENTER" />
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
